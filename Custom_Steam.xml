<?xml version="1.0" encoding="utf-8"?>
<Data>
  <CleanCollection4>
      <Item Name="#[BETA]Steam临时数据">
       <Description>#Steam在运行中产生的缓存数据。（部分规则仅支持默认安装目录）在Github上报告问题>/Sunbangyan233/Dismpp-Custom-Rules</Description>
        <Group>#缓存文件</Group>
       <Scan>
        <Activate>
          <General RootPath="%SystemDrive%\Program Files (x86)\Steam\depotcache"/>
          <General RootPath="%SystemDrive%\Program Files (x86)\Steam\logs"/>
           <General RootPath="%SystemDrive%\Program Files (x86)\Steam\appcache"/>
          <General RootPath="%SystemDrive%\Users\*\AppData\Local\Steam\htmlcache">
        </Activate>
      </Scan>
    </Item>
  </CleanCollection4>
  <SystemOptimization>
  </SystemOptimization>
</Data>
